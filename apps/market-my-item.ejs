 <div class="row">
    <% items.forEach(function(data, index){ %>
        <div class="col-md-4 mb-3 col-12" id="plance">
            <div class="shadown border round">
            	<img src="<%=data.image%>" style="width:100%">
            	<hr>
            	<div class="p-4">
            		Model : <%=data.attributes[0].value%><br>
            		Lever : <%=data.attributes[1].value%><br>
            		Power : <%=data.attributes[2].value%><br>
            	</div>
            	<hr>
            	<div class="p-4">
            		<b>Name</b>
	            	<input type="text" class="form-control name" placeholder="Name" value="<%=data.name%>">
	            	<b>Description</b>
	            	<textarea type="text" class="form-control description" placeholder="Description"><%=data.description%></textarea>
            	</div>
            	<hr>

            	<div class="input-group mb-3 p-4">
				  <input type="text" class="form-control" placeholder="Price" aria-label="Recipient's username" aria-describedby="button-addon2">
				  <div class="input-group-append">
				    <button class="btn btn-primary btn-sm" data-market-sell data-tokenid="<%=data.id%>" type="button" id="button-addon2">SELL</button>
				  </div>
				</div>
            </div>
        </div>
    <%});%>
</div>