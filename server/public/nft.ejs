<h4 class="contractaddress d-flex justify-content-between"></h4>
<hr>
<div class="row">
	<div class="col-6">
		<div class="card">
			<div class="card-body">
			Factory : <%=address.AddressContractNFTFactory%><br>
			CAR : <%=address.AddressContractSmartNFT%><br>
			EXP : <%=address.AddressContractNFTItem%><br>
			</div>
		</div>
	</div>
	<div class="col-6">
		<div class="card">
			<div class="card-body">
				<button class="btn btn-primary" id="setupfatory" data-nft="<%=address.AddressContractSmartNFT%>" data-nft_exp="<%=address.AddressContractNFTItem%>">Settup Factory</button>
				<button class="btn btn-primary" id="setupcar" data-factory="<%=address.AddressContractNFTFactory%>" data-nft_exp="<%=address.AddressContractNFTItem%>">Settup CAR</button>
				<button class="btn btn-primary" id="setupexp" data-factory="<%=address.AddressContractNFTFactory%>">Settup EXP</button>
			</div>
		</div>
	</div>
</div>
<br>
<div class="row">
	<div class="col-6">
		<div class="card">
			<div class="card-body">
				<h5>Create Admin</h5>
				<hr>
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="addAdmin">Create Admin</span>
				</div>
				
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="removeAdmin">Remove Admin</span>
				</div>

			</div>
		</div>
			<br>
		<div class="card">
			<div class="card-body">
				<h5>Create Static User</h5>
				<hr>
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="addStaticUser">Create User</span>
				</div>

				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="removeStaticUser">Remove Users</span>
				</div>
			</div>
		</div>
			<br>
		<div class="card">
			<div class="card-body">
				<h5>Create Generator</h5>
				<hr>
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="addGenerator">Create Generator</span>
				</div>

				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Wallet">
				  <span class="input-group-text" id="removeGenerator">Remove Generator</span>
				</div>
			</div>

		</div>
	</div>

	<div class="col-6">
		<div class="card">
			<div class="card-body">
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Contract NFT Factory">
				  <span class="input-group-text" id="setFactory">Set Factory</span>
				</div>
			</div>
		</div>

			<br>
		<div class="card">
			<div class="card-body">
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Set API URL">
				  <span class="input-group-text" id="setURL">Set URL</span>
				</div>
			</div>
		</div>

			<br>

		<div class="card">
			<div class="card-body">
				<h5>Mint Quality</h5>
				<hr>
				
				<div class="mb-3">
					Lever
					<input type="number" class="form-control" id="generation" placeholder="Generation">
				</div>
				<div class="mb-3">
					Model
					<input type="number" id="quality" class="form-control" placeholder="Quality">
				</div>
				<button class="btn btn-primary" id="mintQuality">Mint Quality</button>
			</div>
		</div>

		<br>
		<div class="card">
			<div class="card-body">
				<h5>Mint</h5>
				<hr>
				<div>
				<input type="text" class="form-control"  placeholder="Name Item" value="CXF 21">
				<button class="btn btn-primary" id="mint">Mint</button>
				</div>
			</div>
		</div>

	</div>

</div>
<hr>
<h4>Item NFT <button class="btn btn-primary" id="mintItem">Mint Item</button></h4>
<div class="row">
	<div class="col-md-6">
		<div class="card">
			<div class="card-body">
				<div class="input-group mt-3">
				  <input type="text" class="form-control" placeholder="Contract NFT Factory">
				  <span class="input-group-text" id="setFactoryItem">Set Factory</span>
				</div>
			</div>
		</div>
	</div>

	<div class="col-md-6">
		<div class="card">
			<div class="card-body">
				<div class="input-group mt-3">
				  <input type="text" class="form-control id" placeholder="Item ID Thumnail">
				  <input type="text" class="form-control exp" placeholder="Item EXP">
				  <span class="input-group-text" id="CreateItem">Create Item</span>
				</div>
			</div>
		</div>
	</div>

</div>